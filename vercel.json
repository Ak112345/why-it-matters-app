{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/content/generate",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/publish/cron",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/errors/detect",
      "schedule": "*/5 * * * *"
    }
  ]
}
